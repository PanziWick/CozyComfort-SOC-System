@model List<Seller.Web.Models.OrderDTO>

<h2>My Orders</h2>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Blanket</th>
            <th>Quantity</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var o in Model)
        {
            <tr>
                <td>@o.Id</td>
                <td>@o.BlanketId</td>
                <td>@o.Quantity</td>
                <td>@o.Status</td>
            </tr>
        }
    </tbody>
</table>
